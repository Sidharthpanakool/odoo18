<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <data>
        <record id="cron_generate_daily_absentees" model="ir.cron">
            <field name="name">Generate Daily Absentees (Attendance)</field>
            <field name="model_id" ref="model_hr_absentees"/>
            <field name="state">code</field>
            <field name="code">model.generate_daily_absentees()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="active">True</field>
        </record>
    </data>

</odoo>
