<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="product_qty.product_card" t-inherit="point_of_sale.ProductCard" t-inherit-mode="extension">
        <xpath expr="//div[hasclass('product-information-tag')]" position="before">
            <div class="product-stock">Qty:
                 <t t-esc="this.props.product.product_qty"/>
            </div>

       </xpath>
    </t>

</templates>