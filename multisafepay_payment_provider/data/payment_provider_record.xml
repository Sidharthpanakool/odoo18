<odoo>

    <record id="payment_provider_multisafepay" model="payment.provider">
        <field name="name">Multisafepay</field>
<!--        <field name="code">multisafepay</field>-->
        <field name="sequence">1</field>
        <field name="image_128" type="base64" file="multisafepay_payment_provider/static/description/icon.png"/>
        <field name="module_id" ref="base.module_multisafepay_payment_provider"/>
        <field name="payment_method_ids"
               eval="[Command.set([
                         ref('multisafepay_payment_provider.payment_method_multisafepay'),
                     ])]"
        />

    </record>


